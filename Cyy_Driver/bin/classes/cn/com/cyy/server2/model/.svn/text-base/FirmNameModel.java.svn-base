package cn.com.cyy.server2.model;

/**
 * 公司名称及Id的模型类
 * 
 * @author hurenji
 * 
 */
public class FirmNameModel {

	private String id;
	private String name;

	public FirmNameModel() {
	}

	public FirmNameModel(String id, String name) {
		this.id = id;
		this.name = name;
	}

	public String getId() {
		return id;
	}

	public void setId(String id) {
		this.id = id;
	}

	public String getName() {
		return name;
	}

	public void setName(String name) {
		this.name = name;
	}

}
