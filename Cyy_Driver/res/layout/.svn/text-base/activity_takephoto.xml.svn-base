<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical" >
  
    <include 
        layout="@layout/every_action_bar1"
        />
    
    <LinearLayout 
        android:layout_width="fill_parent"
        android:layout_height="0dp"
        android:orientation="horizontal"
        android:layout_weight="1"
        >
    <LinearLayout 
       android:layout_width="0dp"
       android:layout_weight="1"
       android:id="@+id/mLLphoto1"
       android:layout_height="wrap_content"
       android:orientation="vertical"
        >
        <TextView 
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="1.事故车牌号"
            />
        <ImageView 
                    android:id="@+id/mIVtakephoto1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
            />
    </LinearLayout>
        <LinearLayout 
       android:layout_width="0dp"
       android:layout_weight="1"
         android:id="@+id/mLLphoto2"
       android:layout_height="wrap_content"
       android:orientation="vertical"
        >
        <TextView 
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="2.事故车牌号"
            />
        <ImageView 
                    android:id="@+id/mIVtakephoto2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
            />
    </LinearLayout>
    
    </LinearLayout>
    <LinearLayout 
        android:layout_width="fill_parent"
        android:layout_height="0dp"
          android:id="@+id/mLLphoto3"
        android:orientation="horizontal"
        android:layout_weight="1"
        >
    <LinearLayout 
       android:layout_width="0dp"
       android:layout_weight="1"
       android:layout_height="wrap_content"
       android:orientation="vertical"
        >
        <TextView 
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="3.事故车牌号"
            />
        <ImageView 
                    android:id="@+id/mIVtakephoto3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
            />
    </LinearLayout>
        <LinearLayout 
       android:layout_width="0dp"
       android:layout_weight="1"
         android:id="@+id/mLLphoto4"
       android:layout_height="wrap_content"
       android:orientation="vertical"
        >
        <TextView 
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="4.事故车牌号"
            />
        <ImageView 
                    android:id="@+id/mIVtakephoto4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
            />
    </LinearLayout>
    
    </LinearLayout>
</LinearLayout>
