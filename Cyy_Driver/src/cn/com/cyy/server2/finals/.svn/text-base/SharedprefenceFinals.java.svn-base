package cn.com.cyy.server2.finals;

import android.content.Context;
import android.content.SharedPreferences;

/**
 * 实例化所有的SharedPrefences
 * @author hurenji
 *
 */
public class SharedprefenceFinals {

	public static SharedPreferences register_shared ;
	private static SharedPreferences connectstatus;
	
	public static void instantiationShared(Context context){
		register_shared = context.getSharedPreferences("register_shared", Context.MODE_PRIVATE);
	}
	
	public static void connectstatusShared(Context context){
		connectstatus = context.getSharedPreferences("status_shared", Context.MODE_PRIVATE);
	}

	
}
